package anavs;

import java.util.concurrent.ExecutorService;
import java.util.concurrent.Executors;

public class ThreadPool {
	Thread[] pool;
	int anzahl;
	int A[][];
	int B[][] = new int[4][4];
	int C[][] = new int[4][4];
	ThreadPool(int anzahl, int A[][], int B[][], int C[][]){
		pool=new Thread[anzahl];
		this.anzahl=anzahl;
		this.A=A;
		this.B=B;
		this.C=C;
	
            Runnable worker = new MyThread(anzahl,A,B,C);
      
    }

}
