package anavs;

public class MyThread extends Thread {
	int nummer;
	int anzahl;
	int A[][];
	int B[][] = new int[4][4];
	int C[][] = new int[4][4];
	
	MyThread(int nummer, int anzahl, int A[][], int B[][], int C[][]){
		this.nummer=nummer-1;
		this.anzahl=anzahl;
		this.A=A;
		this.B=B;
		this.C=C;
	}
    
    public void run() {
        for (int j=nummer/4;j<16;j=j+anzahl){
        for(int i=nummer%4;i<16;i=i+anzahl){
        	System.out.println(A[i][j]);
        }	
        }
    }   

}
